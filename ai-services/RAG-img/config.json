{
    "upload_dir": "uploads",
    "max_file_size": 10485760,
    "supported_formats": [
        ".jpg",
        ".jpeg",
        ".png",
        ".bmp",
        ".gif",
        ".tiff",
        ".webp"
    ],
    "ocr_languages": [
        "vi",
        "en"
    ],
    "embedding_model": "sentence-transformers/all-MiniLM-L6-v2",
    "vector_db_path": "chroma_db",
    "chunk_size": 500,
    "chunk_overlap": 50,
    "similarity_threshold": 0.7,
    "max_results": 10
}